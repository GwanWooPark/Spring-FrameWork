<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- 모두한 4500000 java (Developer) -->
    <bean id="mo" class="com.test03.developer">
        <constructor-arg name="name" value="모두한"/>
        <constructor-arg name="salary" value="4500000"/>
        <property name="dept" value="Developer"/>
    </bean>

    <!-- 안재웅 4500000 java (Engineer) -->
    <bean id="ahn" class="com.test03.Engineer">
        <constructor-arg name="name" value="안재웅"/>
        <constructor-arg name="salary" value="4500000"/>
        <property name="dept" value="Engineer"/>
    </bean>
</beans>